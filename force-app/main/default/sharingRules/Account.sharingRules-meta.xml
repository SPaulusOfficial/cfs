    <sharingCriteriaRules>
        <fullName>Share_Customers</fullName>
        <accessLevel>Edit</accessLevel>
        <accountSettings>
            <caseAccessLevel>None</caseAccessLevel>
            <contactAccessLevel>Edit</contactAccessLevel>
            <opportunityAccessLevel>None</opportunityAccessLevel>
        </accountSettings>
        <description>Provides Read/Write Access to all Customer records (Account / Contact); excludes Staff &amp; MPs</description>
        <label>Share Customers</label>
        <sharedTo>
            <roleAndSubordinates>TfNSW</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>Type</field>
            <operation>equals</operation>
            <value>Analyst,Other,Competitor,Partner,Integrator,Prospect,Investor,Reseller,Customer,Press</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
