            <defaultValue>
                <elementReference>Phone</elementReference>
            </defaultValue>
            <fieldText>Phone</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>&lt;SPAN ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;The format entered does not match the expected format&lt;/FONT&gt;&lt;/SPAN&gt;&lt;DIV ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;• Home/Work: 0X XXXX XXXX&lt;/FONT&gt;&lt;/DIV&gt;&lt;DIV ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;• Mobile: 04XX XXX XXX&lt;/FONT&gt;&lt;/DIV&gt;&lt;DIV ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;• International (Other): &lt;/FONT&gt;&lt;/DIV&gt;&lt;DIV ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;     • (+XXX) XXXX XXXX XXXX, +XXX XXXX XXXX XXXX, &lt;/FONT&gt;&lt;/DIV&gt;&lt;DIV ALIGN=&quot;LEFT&quot;&gt;&lt;FONT FACE=&quot;Arial&quot; STYLE=&quot;font-size:11px&quot; COLOR=&quot;#FF0000&quot; LETTERSPACING=&quot;0&quot; KERNING=&quot;0&quot;&gt;     • +XXX-XXXX-XXXX-XXXX, +XXX.XXXX.XXXX.XXXX.&lt;/FONT&gt;&lt;/DIV&gt;</errorMessage>
                <formulaExpression>IF(OR({!$Profile.Name} = &#39;feedback Profile&#39;,{!$Profile.Name} = &#39;System Administrator&#39;,ISBLANK( {!Phone})), true, 
	IF({!Phone_Type} = {!Home_Phone}, OR((REGEX( {!Phone} , &quot;0\\d{1} \\d{4} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^\\(0\\d{1}\\) \\d{4} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^[1-9]\\d{3} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^[1-9]\\d{7}$&quot;)), (REGEX( {!Phone} , &quot;0\\d{9}$&quot;))),
		IF({!Phone_Type} = {!Mobile_Phone}, OR((REGEX( {!Phone} , &quot;04\\d{2} \\d{3} \\d{3}$&quot;)),(REGEX( {!Phone} , &quot;04\\d{8}$&quot;))),
