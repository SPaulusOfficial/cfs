				IF({!Phone_Type} = {!Work_Phone}, OR((REGEX( {!Phone} , &quot;0\\d{1} \\d{4} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^\\(0\\d{1}\\) \\d{4} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^[1-9]\\d{3} \\d{4}$&quot;)),(REGEX( {!Phone} , &quot;^[1-9]\\d{7}$&quot;)), (REGEX( {!Phone} , &quot;0\\d{9}$&quot;))),false
				)
			)
		)
	)
)</formulaExpression>
