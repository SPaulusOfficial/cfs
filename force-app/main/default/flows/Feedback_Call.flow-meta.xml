    <decisions>
        <name>check_lastname_if_firstname</name>
        <label>check lastname if firstname</label>
        <locationX>648</locationX>
        <locationY>98</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>no_name</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>First_Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Last_Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>TimeOfIncidentParser2</targetReference>
            </connector>
            <label>no name</label>
        </rules>
        <rules>
            <name>firstname_without_lastname</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>First_Name</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Last_Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>LastNameLessThan2</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Error_Message</targetReference>
            </connector>
            <label>firstname without lastname</label>
        </rules>
        <rules>
            <name>firstname_and_lastname_or_only_lastname</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Last_Name</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SearchExistingContact</targetReference>
            </connector>
            <label>firstname and lastname or only lastname</label>
        </rules>
    </decisions>
